---
- name: Parted from Drive dict
  parted:
    device: "/dev/{{item.key}}"
    unit: MiB
  loop: "{{ drive | dict2items }}"
  loop_control:
    label: "{{ item.key }}"
  register: partition
  become: true


- name: Selector
  debug:
    var: "partition.results[0]"
    var: "partition.results[0].disk.dev"
    var: "partition.results[0].partitions"
    var: "partition.results[0].partitions[0].flags"
